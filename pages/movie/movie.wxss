/* pages/movie/movie.wxss */
.top{
	display: flex;
	height: 120rpx;
	border-bottom: 1px solid #ddd;
	text-align: center;
	line-height: 120rpx;
	justify-content: space-around;
	position: relative;
}
.search image{
	margin-top: 40rpx;
	width: 50rpx;
	height: 50rpx;
}
.select-box{
	display: flex;
}
.select-box-item{
	padding-left: 5px;
}
.select-box::after{
	content: "";
	display: block;
	position: absolute;
	bottom: -1px;
	width: 160rpx;
	height: 2px;
	background-color: #ef4238;
	-webkit-transition: left .2s;
	transition: left 02s; 
}
.coming::after{
	left: 380rpx;
}
.movie-item{
	height: 220rpx;
	padding-left: 10rpx;
	display: flex;
	border-bottom: 1px solid #ddd;
}
.subpage{
	position: absolute;
	width: 100%;
	top: 120rpx;
	bottom: 0;
	overflow-y: scroll;
}
.subpage::-webkit-scrollbar{
	width: 0;
	height: 0;
	color: transparent;
}
.mostExperted-list{
	width: 100%;
	height: 330rpx;
	display: flex;
	white-space: nowrap;
	overflow-x: scroll;
	margin-top: 15rpx;
	border-bottom: 20rpx solid #F5F5F5;
}
.mostExperted-list::-webkit-scrollbar{
	width: 0;
	height: 0;
	color: transparent;
}
.mostExperted-item{
	width: 500rpx;
	height: 320rpx;
	box-sizing: border-box;
}
.mostExperted-item:nth-of-type(n+2){
	padding-left: 20rpx;
}
.left{
	width: 20%;
	height: 100%;
	overflow: hidden;
}
.left image{
	display: block;
	margin-top: 15rpx;
	width: 145rpx;
	height: 190rpx;
}
.center{
	height: 100%;
	width: 60%;
	overflow: hidden;
	font-size: 24rpx;
	color: #666;
}
.center .title{
	width: 100%;
	height: 60rpx;
	font-size: 38rpx;
	font-weight: bold;
	padding-left: 15rpx;
	padding-top: 15rpx;
	margin-bottom: 10rpx;
	display: flex;
	color: black;
}
.center .grade{
	width: 100%;
	height: 30rpx;
	font-size: 24rpx;
	padding-left: 15rpx;
	color: #666;
	margin-bottom: 10rpx;
}
.center .grade text{
	color: #faaf00;
	font-weight: bold;
}
.actor{
	width: 100%;
	height: 36rpx;
	color: #666;
	margin-bottom: 10rpx;
}
.actor view text{
	font-size: 24rpx;
	padding-left: 15rpx;
}
.actor view{
	font-size: 24rpx;
	width: 100%;
	height: 100%;
	overflow: hidden;
	white-space: nowrap;
	text-overflow: ellipsis;
}
.num{
	padding-left: 15rpx;
}
.right{
	width: 20%;
	height: 100%;
}
.right navigator{
	display: block;
	width: 100rpx;
	height: 80rpx;
	line-height: 80rpx;
	font-size: 32rpx;
	text-align: center;
	margin-left: 20rpx;
	margin-top: 70rpx;
}
.normal{
	background-color: #f03d37;
	color: white;
	border-radius: 8rpx;
}
.toggle-wish-nav{
	background-color: #faaf00;
}
.pre{
	background-color: #3c9fe6;
}
.most-nav{
	font-size: 24rpx;
	color: #666;
	padding-left: 15rpx;
	padding-top: 15rpx;
}
.most-top{
	width: 100%;
	height: 250rpx;
	position: relative;
}
.most-top .islike{
	width: 50rpx;
	height: 50rpx;
	background-color: rgba(61,63,71,.6);
	border-radius: 0 0 20rpx 0;
	position: absolute;
	left: 0;
	top: 0;
	text-align: center;
}
.most-top .islike image{
	display: block;
	width: 30rpx;
	height: 30rpx;
	overflow: hidden;
	margin-top: 10rpx;
	margin-left: 10rpx;
}
.most-top .want-look{
	color: #faaf00;
	font-weight: bold;
	font-size: 24rpx;
	position: absolute;
	bottom: 4rpx;
	left: 8rpx;
}
.most-top image{
	width: 190rpx;
	height: 100%;
}
.most-bottom .most-title{
	font-size: 24rpx;
	font-weight: bold;
}
.most-bottom .most-time{
	color: #999;
	font-size: 24rpx;
}
.list-nav{
	margin-top: 15rpx;
	font-size: 24rpx;
	padding-left: 15rpx;
	color: #666;
}
.version{
	margin-top: 5rpx;
	margin-left: 4rpx;
	display: inline-block;
	width: 85rpx;
	height: 29rpx;
	background-size: 100% 100%;
}
.v3dimax{
	background-image: url(data:image/png;base64,);
}
.v2dimax{
	background-image: url(data:image/png;base64,);
}
.v3d{
	background-image: url(data:image/png;base64,);
}
	